      proc√©dure PL_CheckAlertes()
      nNombreAlertes	est un entier		= 0
      RqMission		est une requ√™te SQL	= 
      [
      	SELECT Mission.*, EtatMission.Libelle FROM Mission <¬ßcs:3¬ß>
