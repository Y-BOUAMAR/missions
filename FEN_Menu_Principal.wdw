      							SI DateValide(RqCreance.DateNotifCreance) = Vrai ALORS
      								bNonNotifie = Faux
      							FIN
      							SI bNonNotifie = Faux ALORS
      								bConteste	est un bool√©en		= faux
      								RqContest	est une requ√™te SQL	= 
      								[
      									SELECT * FROM Contestation WHERE IDMission={pIDMission}<¬ßcs:3¬ß>
      								]
      								RqContest.pIDMission = RqMission.IDMission
      								SI HEx√©cuteRequ√™te(RqContest) ALORS
      									SI DateValide(RqContest.DateContest)=Vrai ALORS
